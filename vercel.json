{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/myLogicAgent/dist",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/myLogicAgent/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/agent/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/diagram/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/chatbot/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/myLogicAgent/dist/$1"
    }
  ]
}