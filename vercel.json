{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "frontend/myLogicAgent/dist", 
  "installCommand": "npm install",
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/myLogicAgent/dist/$1"
    }
  ]
}