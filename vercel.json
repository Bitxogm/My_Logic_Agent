
{
  "version": 2,
  "builds": [
    {
      "src": "frontend/myLogicAgent/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "backend/package.json", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    // El resto del bloque 'routes' se mantiene igual
    {
      "src": "/api/(.*)",
      "dest": "backend/index.ts" 
    },
    {
      "src": "/agent/(.*)", // AÃ±adimos la ruta '/agent/' que aparece en el error
      "dest": "backend/index.ts"
    },
    {
      "src": "/diagram/(.*)",
      "dest": "backend/index.ts"
    },
    {
      "src": "/chatbot/(.*)",
      "dest": "backend/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/myLogicAgent/dist/$1"
    }
  ]
}