{
  "name": "my_logic_agent",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend/myLogicAgent && npm run dev",
    "dev:browser": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run browser\"",
    "browser": "sleep 3 && google-chrome --incognito --new-window http://localhost:5173",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm install && npm run build",
    "build:frontend": "cd frontend/myLogicAgent && npm run build",
    
    "vercel-build": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bitxogm/My_Logic_Agent.git"
  },
